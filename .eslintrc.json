{
  "env": {
    "browser": true,
    "es2021": true,
    "react-native/react-native": true
  },
  "extends": ["@team-frolog/eslint-config-web"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "createDefaultProgram": true,
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react-native"],
  "rules": { "react/display-name": "off", "react/no-array-index-key": "warn" }
}
